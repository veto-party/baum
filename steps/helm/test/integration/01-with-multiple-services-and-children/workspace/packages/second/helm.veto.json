{
  "is_service": true,
  "variable": {
    "my_service_relation": {
      "type": "scoped",
      "binding": {
        "test": "some_service.origin_name_var"
      },
      "static": true,
      "default": "${test}"
    }
  },
  "binding": {
    "test_service_relation": "my_service_relation"
  },
  "expose": {
    "187": {
      "type": "load-balancer",
      "cors": {
        "self": true,
        "origins": [{
          "source": "examlple.com"
        }, {
          "relative": true,
          "source": "someprefix"
        }]
      }
    }
  }
}
